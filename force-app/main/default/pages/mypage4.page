<apex:page Controller="MyAccount">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!MyAccount.name}" taborderhint="1"/>
                <apex:inputField value="{!MyAccount.My_Test_Field__c}" label="Details" taborderhint="4"/>
                <apex:inputField value="{!MyAccount.Phone}" taborderhint="3"/>
                <apex:inputField value="{!MyAccount.fax}" taborderhint="2"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="create" action="{!save}"/>
            </apex:pageBlockButtons>>
        </apex:pageBlock>
    </apex:form>
</apex:page>